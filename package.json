{
  "name": "technically-tom",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:stats": "ng build --stats-json=true",
    "build:production": "ng build --configuration production --base-href https://www.technicallytom.com/",
    "deploy:production": "npm run build:production && npx angular-cli-ghpages --dir=dist/technically-tom",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers Chrome",
    "coverage": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "ci": "ng test --watch=false --browsers=ChromeHeadless",
    "bundle-report": "webpack-bundle-analyzer dist/technically-tom/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@angular/service-worker": "^16.0.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@ng-bootstrap/ng-bootstrap": "^15.0.1",
    "bootstrap": "^5.3.0",
    "emoji-toolkit": "^7.0.1",
    "lodash-es": "^4.17.21",
    "ngx-bootstrap": "^10.2.0",
    "ngx-markdown": "^16.0.0",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.5",
    "@angular/cli": "~16.0.5",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.195",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.2"
  }
}
